#Vaciamos tabla de enrutamiento y creamos rutas en el archivo /etc/routes, aÃ±adimos reglas iptables nat para SNAT y DNAT forwarding para alcanzar el vpnserver
COMMANDS="DEBIAN_FRONTEND=noninteractive apt install -y iptables-persistent,echo \"ip route del default\" >> /etc/routes,echo \"ip route add 192.168.202.0/24 via 192.168.203.20\" >> /etc/routes,echo \"ip route add default via 192.168.204.1\" >> /etc/routes,echo \"iptables -t nat -A POSTROUTING -o eth0 -j MASQUERADE\">>/etc/routes,echo \"iptables -t nat -A PREROUTING -p udp --dport 1194 -j DNAT --to-destination 192.168.201.100:1194\">>/etc/routes"

